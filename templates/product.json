            "custom_liquid_jFmVYj_meld": {
              "type": "custom-liquid",
              "name": "Decide Color Guide",
              "disabled": true,
              "settings": {
                "custom_liquid": "{%- assign help_id = 'help-decide-' | append: block.id | default: 'help-decide-1' -%}\n\n<section id=\"{{ help_id }}\" class=\"na-help-decide\" aria-label=\"Help me choose popup\">\n  <!-- Trigger -->\n  <button class=\"na-btn\" type=\"button\" data-open>\n    Which color palette? <span class=\"na-underline\">Help me decide</span>\n  </button>\n\n  <!-- Modal -->\n  <div class=\"na-modal\" hidden>\n    <div class=\"na-backdrop\" data-close tabindex=\"-1\" aria-hidden=\"true\"></div>\n\n    <div class=\"na-dialog\" role=\"dialog\" aria-modal=\"true\" aria-labelledby=\"{{ help_id }}-title\">\n      <header class=\"na-header\">\n        <div>\n          <h3 id=\"{{ help_id }}-title\">Which kit is right for my photo?</h3>\n          <p class=\"na-subtext\">The more colors, the more details.</p>\n        </div>\n        <button class=\"na-close\" type=\"button\" data-close aria-label=\"Close\">×</button>\n      </header>\n\n      <div class=\"na-scroll\">\n        <div class=\"na-content\">\n          <!-- Card 1 -->\n          <article class=\"na-card\">\n            <div class=\"na-img-slot\"></div>\n            <div class=\"na-card-body\">\n              <h4>24 Colors – Quick & Beginner-Friendly</h4>\n              <ul class=\"na-list\">\n                <li>Simple layout with fewer details.</li>\n                <li>Perfect for first-time painters or a fast project.</li>\n                <li>Great if you want to finish quickly with satisfying results.</li>\n              </ul>\n            </div>\n          </article>\n\n          <!-- Card 2 -->\n          <article class=\"na-card\">\n            <div class=\"na-img-slot\"></div>\n            <div class=\"na-card-body\">\n              <h4>36 Colors – Balanced & Versatile</h4>\n              <ul class=\"na-list\">\n                <li>More detail and shading than 24 colors.</li>\n                <li>A well-rounded choice for most photos and painters.</li>\n                <li>Medium complexity without feeling overwhelming.</li>\n              </ul>\n            </div>\n          </article>\n\n          <!-- Card 3 -->\n          <article class=\"na-card\">\n            <div class=\"na-img-slot\"></div>\n            <div class=\"na-card-body\">\n              <h4>48 Colors – Maximum Detail</h4>\n              <ul class=\"na-list\">\n                <li>The most detailed option with fine shading.</li>\n                <li>Ideal for experienced painters seeking a challenge.</li>\n                <li>Provides rich color depth and subtle transitions.</li>\n              </ul>\n            </div>\n          </article>\n\n          <div class=\"na-actions\">\n            <button class=\"na-cta\" type=\"button\" data-close>Got it – continue</button>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n\n  <style>\n    #{{ help_id }} {\n      --na-c: #2A4759;\n      --na-c2: #444;\n      --na-bg: #fff;\n      --na-border: #e5e7eb;\n      --na-shadow: 0 10px 30px rgba(0,0,0,0.15);\n    }\n\n    .na-btn {\n      font: inherit; background: none; border: 0; padding: .25rem 0; cursor: pointer;\n      color: var(--na-c); display: inline-flex; gap: .4rem; align-items: center;\n    }\n    .na-btn .na-underline { text-decoration: underline; text-underline-offset: 2px; }\n\n    .na-modal[hidden] { display: none; }\n    .na-modal {\n      position: fixed; inset: 0; z-index: 9999;\n      display: flex; align-items: center; justify-content: center;\n    }\n    .na-backdrop {\n      position: absolute; inset: 0; background: rgba(0,0,0,.35);\n    }\n\n    .na-dialog {\n      background: var(--na-bg); border-radius: 16px;\n      box-shadow: var(--na-shadow); overflow: hidden; z-index: 1;\n      display: flex; flex-direction: column;\n      max-height: 90vh; width: 92%; max-width: 720px;\n    }\n\n    .na-header {\n      display: flex; justify-content: space-between; align-items: flex-start;\n      padding: 1rem 1.25rem; border-bottom: 1px solid var(--na-border);\n    }\n    .na-header h3 {\n      margin: 0; font-size: 1.2rem; color: var(--na-c);\n    }\n    .na-subtext {\n      font-size: 0.875rem; color: #6b7280; margin: 0.25rem 0 0;\n    }\n    .na-close {\n      background: none; border: 0; font-size: 1.5rem;\n      color: #6b7280; cursor: pointer;\n      line-height: 1;\n    }\n\n    .na-scroll {\n      overflow-y: auto; padding: 1rem; flex: 1;\n    }\n    .na-content {\n      display: flex; flex-direction: column; gap: 1rem;\n    }\n\n    .na-card {\n      border: 1px solid var(--na-border);\n      border-radius: 12px; overflow: hidden;\n      background: #fff;\n    }\n    .na-img-slot {\n      min-height: 140px; background: #f3f4f6;\n      display: flex; justify-content: center; align-items: center;\n      font-size: 0.875rem; color: #94a3b8;\n    }\n    .na-img-slot::after {\n      content: \"Add image here\";\n    }\n\n    .na-card-body {\n      padding: 1rem;\n    }\n    .na-card h4 {\n      font-size: 1rem; margin: 0 0 0.5rem;\n      color: var(--na-c2);\n    }\n    .na-list {\n      margin: 0; padding-left: 1.25rem;\n    }\n    .na-list li {\n      margin: 0.25rem 0;\n      font-size: 0.95rem;\n    }\n\n    .na-actions {\n      display: flex; justify-content: flex-end; margin-top: 0.5rem;\n    }\n    .na-cta {\n      background: var(--na-c); color: #fff;\n      border: 0; padding: 0.6rem 1rem;\n      border-radius: 8px; cursor: pointer;\n      font-weight: 600;\n    }\n    .na-cta:hover {\n      background: var(--na-c2);\n    }\n\n    @media (min-width: 640px) {\n      .na-card {\n        display: grid;\n        grid-template-columns: 200px 1fr;\n      }\n      .na-img-slot { min-height: 100%; }\n    }\n  </style>\n\n  <script>\n    (function(){\n      var root = document.getElementById('{{ help_id }}');\n      if (!root) return;\n      var modal = root.querySelector('.na-modal');\n      var openBtn = root.querySelector('[data-open]');\n      var closeEls = root.querySelectorAll('[data-close]');\n      var backdrop = root.querySelector('.na-backdrop');\n\n      function open() {\n        modal.hidden = false;\n        document.body.style.overflow = 'hidden';\n        trapFocus();\n      }\n      function close() {\n        modal.hidden = true;\n        document.body.style.overflow = '';\n        document.removeEventListener('keydown', esc);\n      }\n      function esc(e) {\n        if (e.key === 'Escape') close();\n      }\n      function trapFocus() {\n        var focusables = modal.querySelectorAll('button, [href], summary, details, input, select, textarea, [tabindex]:not([tabindex=\"-1\"])');\n        if (focusables.length) focusables[0].focus();\n        document.addEventListener('keydown', esc);\n      }\n\n      openBtn && openBtn.addEventListener('click', open);\n      closeEls.forEach(el => el.addEventListener('click', close));\n      backdrop && backdrop.addEventListener('click', close);\n    })();\n  </script>\n</section>"
